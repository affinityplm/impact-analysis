{
    "ManufacturingProcedure":{
        "Variables":["RequiredPartBOMs","RequiredParts","RequiredDocuments"],
        "Template":{
            "IncludedBOMs":[
                {
                    "Type":"PART",
                    "Number|Revision":"RequiredPartBOMs"
                }
            ],
            "IncludedObjects":[
                {   
                    "Type":"PART",
                    "Number|Revision":"RequiredParts"
                },{
                    "Type":"DOCUMENT",
                    "Number|Revision":"RequiredDocuments"
                }
            ],
            "ExcludedBOMs":[],
            "ExcludedObjects":[]
        }
    },
    "EngineeringDesign":{
        "Variables":["Number","Revision","RequiredParts","RequiredDocuments"],
        "Template":{
            "IncludedBOMs":[
                {
                    "Combine":{"RequiredPartBOMs":{"Number":"Number","Revision":"Revision"}},
                    "Type":"PART",
                    "Number|Revision":"RequiredPartBOMs"
                }
            ],
            "IncludedObjects":
            [
                {   
                    "Type":"PART",
                    "Number|Revision":"RequiredParts"
                },{
                    "Type":"DOCUMENT",
                    "Number|Revision":"RequiredDocuments"
                }
            ],
            "ExcludedBOMs":[],
            "ExcludedObjects":[]
        }
    }
}
